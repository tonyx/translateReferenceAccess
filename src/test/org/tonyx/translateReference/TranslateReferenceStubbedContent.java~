            "                    </div>\n" +

            "                </div>\n" +
            "                </div>\n" +
            "                \n" +
            "<script>\n" +
            "\tcharCount();\n" +
            "    insertWaterMark();    \n" +
            "    function ismaxlength(obj){\n" +
            "        var mlength=obj.getAttribute? parseInt(obj.getAttribute(\"maxlength\")) : \"\"\n" +
            "        if (obj.getAttribute && obj.value.length>mlength)\n" +
            "            obj.value=obj.value.substring(0,mlength)\n" +
            "    }\n" +
            "    function clearText(obj){\n" +
            "        if(obj.value == \"Enter 140 characters or less for translation\"){\n" +
            "            obj.value = \"\";\n" +
            "            obj.style.color = \"#000000\";\n" +
            "            obj.focus();\n" +
            "        }\n" +
            "    }\n" +

            "//    Symbols module\n" +

            "  var _last_focus_elem =null;\n" +
            "     $j(\".tap_newtext\").focusin(function(){_last_focus_elem = this;});\n" +
            "     function initKeyboardWidget(){\n" +
            "         var key_list = new Array(\"&Agrave;\", \"&agrave;\", \"&Aacute\", \"&aacute;\",\n" +
            "                 \"&Acirc;\", \"&acirc;\", \"&Atilde;\", \"&atilde;\", \"&Auml;\", \"&auml;\", \"&Aring;\",\n" +
            "                 \"&aring;\", \"&Ccedil;\", \"&ccedil;\", \"&Egrave;\", \"&egrave;\", \"&Eacute;\",\n" +
            "                 \"&eacute;\", \"&Ecirc;\", \"&ecirc;\", \"&Euml;\", \"&euml;\", \"&Igrave;\", \"&igrave;\",\n" +
            "                 \"&Iacute;\", \"&iacute;\", \"&Icirc;\", \"&icirc;\", \"&#x143;\", \"&#x144;\", \"&Ntilde;\",\n" +
            "                 \"&ntilde;\", \"&Ograve;\", \"&ograve;\", \"&Oacute;\",\"&oacute;\", \"&Ouml;\", \"&ouml;\",\n" +
            "                 \"&Ocirc;\", \"&ocirc;\", \"&Ugrave;\", \"&ugrave;\", \"&Uacute;\", \"&uacute;\", \"&Uuml;\",\n" +
            "                 \"&uuml;\", \"&Ucirc;\", \"&ucirc;\", \"&Yacute;\", \"&yacute;\", \"&Yuml;\", \"&yuml;\" );\n" +
            "         var key_string = '';\n" +
            "         for(k in key_list){\n" +
            "             key_string += '<button class=\"keyboard_symbol\" style=\"display: -moz-inline-stack;width: 25px; margin: 0;\" tabindex=1>'+key_list[k]+'</button>';\n" +
            "         }\n" +
            "         // have a hidden dummy button that prevents IE from focusing on a button in the window\n" +
            "         if (/MSIE (\\d+\\.\\d+);/.test(navigator.userAgent)){\n" +
            "             key_string += '<button style=\"height:0;width:0;margin:0;padding:0;border:0;\" tabindex=0></button>';\n" +
            "         }\n" +

            "          var keyboard = '<div id=\"tooltip_keyboard\" class=\"tooltip_keyboard\"title=\"Symbols & accents\"><p>Click on any character below to insert it into translate box.</p><p>'+key_string+'</p></div>';\n" +
            "         $j(\"body\").append(keyboard);\n" +
            "         // dialog starts\n" +
            "         var offsetleft= $j(\".symbl\").position().left+383;\n" +
            "         var offsettop= $j(\".symbl\").position().top;\n" +
            "         $j(\".symbl\").click(function(){\n" +
            "             $j(\".tooltip_keyboard\").dialog({autoOpen: false, resizable: true, show: \"blind\", hide: \"blind\",position:[offsetleft,180],dialogClass: 'keyboard-dialog' });\n" +
            "             $j(\".tooltip_keyboard\").dialog(\"open\");\n" +



            "//        $j(\".tooltip_keyboard\").dialog( 'option', 'position', [offsetleft,0]);\n" +
            "             return true;\n" +
            "   });\n" +
            "         $j('.keyboard_symbol').click(function(){\n" +
            "             //focus out of box do the validation\n" +
            "             if(_last_focus_elem){\n" +
            "                 var caret_position = getCaret(_last_focus_elem);\n" +
            "                 var textval = _last_focus_elem.value;\n" +
            "                 var left_text  = textval.substring(0, caret_position);\n" +
            "                 var right_text = textval.substring(caret_position, textval.length );\n" +
            "                 _last_focus_elem.value = left_text + this.innerHTML + right_text;\n" +
            "                 charCount();\n" +
            "                 return ismaxlength(this);\n" +
            "             }\n" +
            "             else {\n" +
            "                 alert('Please place your cursor in the field you want to edit.');\n" +
            "             }\n" +
            "         });\n" +
            "     }\n" +
            "     $j(document).ready(function(){\n" +
            "         initKeyboardWidget();\n" +
            "     });\n" +
            "     function getCaret(el) {\n" +
            "         if (el.selectionStart) {\n" +
            "             return el.selectionStart;\n" +
            "         } else if (document.selection) {\n" +
            "             el.focus();\n" +
            "             var r = document.selection.createRange();\n" +
            "             if (r == null) {\n" +
            "                 return 0;\n" +
            "             }\n" +
            "             var re = el.createTextRange(),\n" +
            "                     rc = re.duplicate();\n" +
            "             re.moveToBookmark(r.getBookmark());\n" +
            "             rc.setEndPoint('EndToStart', re);\n" +
            "             return rc.text.length;\n" +
            "         }\n" +
            "         return 0;\n" +
            " }\n" +
            "</script>\n" +
            "     \n" +
            "<script type=\"text/javascript\" src=\"https://apis.google.com/js/plusone.js\"></script>\n" +













