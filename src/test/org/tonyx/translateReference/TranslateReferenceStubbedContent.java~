            "        var query=\"\";\n" +
            "        if (Cookie.isEnabled()) {\n" +
            "            var date = new Date();\n" +
            "            date.setTime(date.getTime() + 172800000);\n" +
            "            Cookie.setCookie(\"translangpair\",source+\"-\"+destination,date,\"/\",\"reference.com\");\n" +
            "        }\n" +
            "        var destlang =  $j('#dst option:selected').text();\n" +
