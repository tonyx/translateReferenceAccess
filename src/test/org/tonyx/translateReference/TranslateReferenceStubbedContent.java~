            "            }\n" +
            "            else{\n" +
            "                document.getElementById('tg2').innerHTML= response.data.translations[0].translatedText;\n" +
            "            }\n" +
            "        }\n" +
            "        if (languageFrom != languageTo){\n" +
            "        var s = document.createElement(\"script\");\n" +
            "        s.src= \"https://www.googleapis.com/language/translate/v2?key=AIzaSyDfE-B0lz3VGyNMPahlfUqDVkNRP8jGTuA&source=\"+languageFrom+\"&target=\"+languageTo+\"&callback=translateText&q=\" + text;\n" +
            "        document.getElementsByTagName(\"head\")[0].appendChild(s);\n" +
            "         }\n" +
            "        else{\n" +
            "          document.getElementById('tg2').innerHTML= text;\n" +
            "        }\n" +
            "    }\n" +
