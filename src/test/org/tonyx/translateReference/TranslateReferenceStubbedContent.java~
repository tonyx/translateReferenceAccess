            "<script type=\"text/javascript\">\n" +
            "    \n" +
            "    var languageFrom = 'en';\n" +
            "    var languageTo = 'it';\n" +
            "    function translateGoogle(src) {\n" +
            "        spk(this,{lk:'b9lp6p',en:'transge',io:'0',b:'trans',tp:'mid',m:'transge',pt:'Translate+Google+Expand'});\n" +
            "        document.getElementById('tgl').style.display=\"block\";\n" +
